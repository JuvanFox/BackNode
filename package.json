{
  "name": "back",
  "version": "1.0.0",
  "description": "An Amazing Capacitor App",
  "author": {
    "name": "",
    "email": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WeAreKins/-back.git"
  },
  "license": "MIT",
  "main": "build/src/index.js",
  "scripts": {
    "build": "tsc && electron-rebuild && cp -R services ./build/src",
    "electron:start-live": "node ./live-runner.js",
    "electron:start": "npm run build && electron --inspect=5858 ./",
    "electron:pack": "npm run build && electron-builder build --dir -c ./electron-builder.config.json",
    "electron:make": "npm run build && electron-builder build -c ./electron-builder.config.json -p always",
    "electron:pack:windows": "npm run build && electron-builder build --win --x64 --ia32 --arm64 -c ./electron-builder.config.json -p always",
    "electron:pack:linux": "npm run build && electron-builder build --mac -c ./electron-builder.config.json -p never",
    "electron:pack:mac": "npm run build && electron-builder build --linux -c ./electron-builder.config.json -p never",
    "release": "electron-builder"
  },
  "dependencies": {
    "@capacitor-community/electron": "^4.1.0",
    "@capacitor/app": "1.1.1",
    "@capacitor/cli": "3.5.0",
    "@capacitor/core": "3.5.0",
    "@capacitor/haptics": "1.1.4",
    "@capacitor/keyboard": "1.2.2",
    "@capacitor/status-bar": "1.0.8",
    "archiver": "^5.3.1",
    "chokidar": "~3.5.2",
    "electron-is-dev": "~2.0.0",
    "electron-serve": "~1.1.0",
    "electron-unhandled": "~3.0.2",
    "electron-updater": "~4.3.9",
    "electron-window-state": "~5.0.3"
  },
  "devDependencies": {
    "electron": "^14.0.0",
    "electron-builder": "~22.11.7",
    "electron-rebuild": "^3.2.3",
    "typescript": "~4.3.5"
  },
  "keywords": [
    "capacitor",
    "electron"
  ]
}
